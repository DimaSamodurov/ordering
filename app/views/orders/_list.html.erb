<table>
  <thead>
  <tr>
    <th>User</th>
    <th>Note</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% orders.each do |order| %>
      <tr>
        <td><%= order.created_at %></td>
        <td><%= order.status %></td>
        <td><%= order.order_items.map{ |item|item.product.name}.join(', ') %></td>
      </tr>
  <% end %>
  </tbody>
</table>
